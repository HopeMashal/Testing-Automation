<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Check Element Task">
  <parameter name="Browser" value="chrome"/>
  <parameter name="CSVPath" value="./src/files/input.csv"/>
  <test name="Testing Element - Create">
    <classes>
      <class name="elementtask.CreateElementTask"/>
    </classes>
  </test>
  <test name="Testing Element - View">
    <classes>
      <class name="elementtask.ViewElementTask"/>
    </classes>
  </test>
  <test name="Testing Element - Edit">
    <classes>
      <class name="elementtask.EditElementTask"/>
    </classes>
  </test>
  <test name="Testing Element - Delete">
    <classes>
      <class name="elementtask.DeleteElementTask"/>
    </classes>
  </test>
</suite>