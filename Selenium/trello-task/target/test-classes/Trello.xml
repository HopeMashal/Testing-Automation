<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Trello Task">
  <parameter name="CSVPath" value="./src/files/input.csv"/>
  <test name="Trello Task - Chrome">
    <parameter name="Browser" value="chrome"/>
    <classes>
      <class name="trello.TrelloTask"/>
    </classes>
  </test>
  <test name="Trello Task - Edge">
    <parameter name="Browser" value="edge"/>
    <classes>
      <class name="trello.TrelloTask"/>
    </classes>
  </test>
  <test name="Trello Task - FireFox">
    <parameter name="Browser" value="firefox"/>
    <classes>
      <class name="trello.TrelloTask"/>
    </classes>
  </test>
</suite>